{
    "name": "easy-tag-for-liquidjs",
    "version": "1.0.0",
    "description": "simple interface for creating tags for Liquidjs",
    "main": "./cjs/index.js",
    "module": "./esm/index.js",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "test": "ava",
        "prepare": "npm run build && npm test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/UpperCod/easy-tag-for-liquidjs.git"
    },
    "author": "UpperCod",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/UpperCod/easy-tag-for-liquidjs/issues"
    },
    "homepage": "https://github.com/UpperCod/easy-tag-for-liquidjs#readme",
    "dependencies": {
        "liquidjs": "^9.15.0"
    },
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^9.0.0",
        "ava": "^3.12.1",
        "builtin-modules": "^3.1.0",
        "esm": "^3.2.25",
        "rollup": "^2.26.11"
    },
    "typings": "types.d.ts",
    "ava": {
        "require": [
            "esm"
        ],
        "files": [
            "test/**/*.test.js"
        ]
    },
    "exports": {
        "./": {
            "import": "./ems/",
            "require": "./cjs/"
        }
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": false
    }
}
